                "xml": {
                    "name": "uuid"
                }
            },
            "Contact": {
                "type": "object",
                "properties": {
                    "userName": {
                        "type": "string"
                    },
                    "phoneNumber": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    }
                },
                "xml": {
                    "name": "contacts"
                }
            },
            "Contacts": {
                "type": "object",
                "properties": {
                    "contacts": {
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/Contact"
                        }
                    }
                }
            }
        }
    }
