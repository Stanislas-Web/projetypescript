    "paths": {
        "/users": {
            "get": {
                "summary": "Obtenir quelques Users",
                "description": "Renvoie une liste contenant tous les Users.",
                "responses": {
                    "200": {
                        "description": "La liste des Users"
                    }
                }
            },
            "post": {
                "summary": "Ajouter quelques Users",
                "operationId": "addUser",
                "requestBody": {
                    "description": "ajouter un User dans l'api",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        },
                        "application/xml": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "405": {
                        "description": "Invalid input",
                        "content": {}
                    }
                },
                "x-codegen-request-body-name": "body"
            }
        },
        "/users/{UserId}": {
            "get": {
                "summary": "Recuperer un User",
                "operationId": "getUserById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "La recherche d'un User par rapport à l'Id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/xml": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            },
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid username supplied",
                        "content": {}
                    },
                    "404": {
                        "description": "User not found",
                        "content": {}
                    }
                }
            },
            "put": {
                "summary": "Modifier quelques Users",
                "description": "Modifier un User",
                "operationId": "updateUser",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Modifier un User",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Modifier un object",
                    "content": {
                        "*/*": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "400": {
                        "description": "Invalid user supplied",
                        "content": {}
                    },
                    "404": {
                        "description": "User not found",
                        "content": {}
                    }
                },
                "x-codegen-request-body-name": "body"
            },
            "delete": {
                "summary": "Supprimer quelques Users",
                "description": "La suppression du User par rapport a l'Id.",
                "operationId": "deleteUser",
                "parameters": [
                    {
                        "name": "Id",
                        "in": "path",
                        "description": "Supprimer un conatact par rapport a l'id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Invalid Id supplied",
                        "content": {}
                    },
                    "404": {
                        "description": "User not found",
                        "content": {}
                    }
                }
            }
        }
    },
    "components": {
