            "User": {
                "type": "object",
                "properties": {
                    "nom": {
                        "type": "string"
                    },
                    "photo": {
                        "type": "string"
                    },
                    "localisation": {
                        "type": "string"
                    },
                    "deviceMark": {
                        "type": "string"
                    },
                    "osVersion": {
                        "type": "string"
                    },
                    "deviceSerial": {
                        "type": "string"
                    },
                    "deviceRegistration": {
                        "type": "string"
                    }
                },
                "xml": {
                    "name": "Users"
                }
            },
            "Users": {
                "type": "object",
                "properties": {
                    "Users": {
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/User"
                        }
                    }
                }
            }
        }
